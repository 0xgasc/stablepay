{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/db-health",
      "destination": "/api/db_health"
    },
    {
      "source": "/",
      "destination": "/public/index.html"
    },
    {
      "source": "/public/(.*)",
      "destination": "/public/$1"
    },
    {
      "source": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico))",
      "destination": "/public/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ]
}