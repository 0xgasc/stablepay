{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    },
    {
      "source": "/health/:path*",
      "destination": "/api"
    },
    {
      "source": "/public/:path*",
      "destination": "/:path*"
    },
    {
      "source": "/checkout",
      "destination": "/crypto-pay.html"
    }
  ]
}