{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/health.*",
      "dest": "dist/index.js"
    },
    {
      "src": "/api/.*",
      "dest": "dist/index.js"
    },
    {
      "src": "/(admin|crypto-pay|demo-store|login|enterprise-admin|onboard-merchant|pricing|dashboard|widget-demo|developers|store|demo-integration|trust|signup)\\.html",
      "dest": "/public/$1.html"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|svg|ico|json))",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}