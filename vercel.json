{
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    },
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/signup",
      "destination": "/signup.html"
    },
    {
      "source": "/pricing",
      "destination": "/pricing.html"
    },
    {
      "source": "/enterprise-admin",
      "destination": "/enterprise-admin.html"
    },
    {
      "source": "/checkout",
      "destination": "/crypto-pay.html"
    },
    {
      "source": "/api/cron/:path*",
      "destination": "/api/cron/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "/api"
    },
    {
      "source": "/health/:path*",
      "destination": "/api"
    }
  ]
}
